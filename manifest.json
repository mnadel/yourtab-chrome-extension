{
    "name": "Your Tab",
    "description": "Own your new tabs.",
    "version": "0.2.3",
    "manifest_version": 2,
    "options_ui": {
        "page": "options.html",
        "open_in_tab": false
    },
    "chrome_url_overrides": {
        "newtab": "yourtab.html"
    },
    "icons": {
        "16": "tab16.png",
        "48": "tab48.png",
       "128": "tab128.png"
    },
    "permissions": [
        "tabs",
        "storage",
        "bookmarks",
        "https://api.darksky.net/*"
    ],
    "background": {
        "persistent": false,
        "scripts": [
            "weather.js"
        ]
    }
}
